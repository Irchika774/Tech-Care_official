{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Permissions-Policy",
                    "value": "geolocation=(), microphone=(), camera=(), payment=(), usb=()"
                }
            ]
        }
    ]
}